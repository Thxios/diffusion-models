{
  "output_dir": "outputs/rf_mnist_base",
  "wandb_run_name": "rf_mnist_base",
  "max_steps": 200000,
  "logging_steps": 50,
  "eval_steps": 1000,
  "save_steps": 10000,
  "eval_n_examples": 40,
  "guidance_scale": 1.0,
  "batch_size": 128,
  "lr": 0.0002,
  "optimizer": "adamw",
  "clip_grad_norm": 1.0,
  "use_ema": true,
  "ema_inv_gamma": 1.0,
  "ema_power": 0.75,
  "dataset": "mnist",
  "dataset_dir": "~/datasets",
  "augmentations": [],
  "dataloader_num_workers": 2,
  "device": "cuda:6",
  "seed": 42,
  "p_uncond": 0.2,
  "model_type": "unet",
  "model_cfg": {
    "in_channels": 1,
    "out_channels": 1,
    "down_block_types": [
      "UNetDownBlock",
      "UNetDownBlock",
      "UNetDownBlock"
    ],
    "up_block_types": [
      "UNetUpBlock",
      "UNetUpBlock",
      "UNetUpBlock"
    ],
    "block_out_channels": [
      64,
      128,
      256
    ],
    "n_blocks_per_layer": 2,
    "mid_attention": false,
    "norm_groups": 32,
    "activation": "silu",
    "time_encoding_dim": 64,
    "embedding_dim": 64,
    "embedding_mixing": "scale_shift",
    "n_class_embeddings": 10,
    "dropout": 0.0,
    "downsample_use_conv": true,
    "upsample_use_conv": true
  },
  "scheduler_type": "rectified_flow",
  "scheduler_cfg": {
    "n_steps": 1000
  },
  "sampler_type": "euler",
  "sampler_cfg": {
    "n_steps": 50,
    "pbar": true
  }
}
