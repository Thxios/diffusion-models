{
  "output_dir": "outputs/rf_cifar10_long2",
  "wandb_run_name": "rf_cifar10_long2",

  "max_steps": 1000000,
  "logging_steps": 50,
  "eval_steps": 5000,
  "save_steps": 25000,
  "eval_n_examples": 40,
  "guidance_scale": 1.0,

  "fid_eval_steps": 25000,
  "fid_ema": true,
  "fid_reference_dataset": "cifar10-train",
  "fid_n_examples": 10000,
  "generation_batch_size": 256,
  "inception_batch_size": 512,
  "adjust_fid_n": true,
  "fid_adjust_subsets": [4000, 6000, 8000, 10000],

  "batch_size": 128,
  "lr": 0.0001,
  "optimizer": "adamw",
  "clip_grad_norm": 1.0,
  "use_ema": true,
  "ema_inv_gamma": 1.0,
  "ema_power": 0.6667,
  "dataset": "cifar10",
  "dataset_dir": "~/datasets",
  "augmentations": ["RandomHorizontalFlip"],
  "dataloader_num_workers": 2,

  "device": "cuda:1",
  "seed": 42,
  
  "p_uncond": 0.2,
  "model_type": "unet",
  "model_cfg": {
    "in_channels": 3,
    "out_channels": 3,
    "down_block_types": [
      "UNetDownBlock",
      "UNetDownBlock",
      "UNetDownBlock"
    ],
    "up_block_types": [
      "UNetUpBlock",
      "UNetUpBlock",
      "UNetUpBlock"
    ],
    "block_out_channels": [
      128,
      256,
      512
    ],
    "n_blocks_per_layer": 2,
    "mid_attention": true,
    "n_dim_attention_head": 8,
    "norm_groups": 32,
    "activation": "silu",
    "time_encoding_dim": 128,
    "embedding_dim": 128,
    "embedding_mixing": "scale_shift",
    "n_class_embeddings": 10,
    "dropout": 0.0,
    "downsample_use_conv": true,
    "upsample_use_conv": true
  },
  "scheduler_type": "rectified_flow",
  "scheduler_cfg": {
    "n_steps": 1000
  },
  "sampler_type": "euler",
  "sampler_cfg": {
    "n_steps": 50,
    "pbar": true
  }
}
